<AnchorPane prefHeight="600.0" prefWidth="800.0" stylesheets="@Estilos.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.proyectonuevo.Tablas.CubreBocasController">
    <children>
        <TableView layoutX="50.0" layoutY="80.0" prefHeight="280.0" prefWidth="459.0">
            <columns>
                <TableColumn prefWidth="75.0" text="No" />
                <TableColumn minWidth="50.0" prefWidth="120.0" text="Tiempo llegada" />
                <TableColumn prefWidth="120.0" text="Tiempo Atención" />
            <TableColumn prefWidth="124.0" text="Tiene CubreBocas" />
            </columns>
        </TableView>
        <GridPane layoutX="49.0" layoutY="364.0" prefHeight="181.0" prefWidth="304.0">
            <columnConstraints>
                <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" maxWidth="145.0" minWidth="10.0" prefWidth="129.0" />
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="170.0" minWidth="10.0" prefWidth="170.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <TextField fx:id="txtNombre" alignment="CENTER" GridPane.columnIndex="1" />
                <Label text="Tiempo De Llegada" />
                <Label alignment="CENTER_RIGHT" contentDisplay="CENTER" text="Tiempo De Atención" GridPane.rowIndex="1" />
                <TextField fx:id="txtNoControl" alignment="CENTER" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                <Label alignment="CENTER_RIGHT" contentDisplay="RIGHT" prefHeight="17.0" prefWidth="137.0" text="Insetar" GridPane.rowIndex="3" />
                <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="100.0" text="Ingresar" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Label alignment="CENTER_RIGHT" contentDisplay="CENTER" text="Con CubreBoca" GridPane.rowIndex="2" />
            <ComboBox prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            </children>
        </GridPane>
        <Button layoutX="598.0" layoutY="195.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="150.0" text="Simulación" />
        <Button layoutX="23.0" layoutY="12.0" mnemonicParsing="false" onAction="#regresar" text="Atras" />

    </children>
